@model Kund

<h1>Lägg till ny kund</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Namn">Namn</label>
        <input asp-for="Namn" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Adress">Adress</label>
        <input asp-for="Adress" class="form-control" />
    </div>
   
    <div class="form-group">
        <label asp-for="Telefonnummer">Telefonnummer</label>
        <input asp-for="Telefonnummer" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Email">Email</label>
        <input asp-for="Email" class="form-control" />
    </div>
     <div class="form-group">
        <label asp-for="Bilmodell">Bilmodell</label>
        <input asp-for="Bilmodell" class="form-control" />
    </div>
      <div class="form-group">
        <label asp-for="Årsmodell">Årsmodell</label>
        <input asp-for="Årsmodell" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>